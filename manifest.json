{
    "name": "Etebase",
    "id": "etebase",
    "packaging_format": 1,
    "description": {
        "en": "Browsing, reading and downloading eBooks using a Calibre database",
        "fr": "Explorer, lire et télécharger des eBooks à partir d'une base de données Calibre"
    },
    "version": "0.96.19~ynh8",
    "url": "https://github.com/etesync/server",
    "upstream": {
        "version": "0.6.19",
        "license": "GPL-3.0-only",
        "admindoc": "https://github.com/janeczku/calibre-web/wiki",
        "code": "https://github.com/etesync/server"
    },
    "license": "GPL-3.0-only",
    "maintainer": {
        "name": "Krakinou",
        "email": "misterl56@hotmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/etebase",
                "default": "/etebase"
            }
        ]
    }
}
